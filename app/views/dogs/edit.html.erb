<h1>Edit Dog:</h1>
 <%= form_tag "/dog_store/#{@dogs.id}", method: :patch do %>
   <div class="form-group">
     <%= label_tag :breed %>
     <%= text_field_tag :breed, @dogs.breed, class: "form-control"  %>
   </div>
   <div class="form-group">
     <%= label_tag :sex  %>
     <%= text_field_tag :sex, @dogs.sex, class: "form-control"  %>
   </div>
   <div class="form-group">
     <%= label_tag :age  %>
     <%= text_field_tag :age, @dogs.age, class: "form-control"  %>
   </div>
   <div class="form-group">
     <%= label_tag :price  %>
     <%= text_field_tag :price, @dogs.price, class: "form-control"  %>
   </div>
   <%= submit_tag "Update dog", class: "btn btn-default" %>
 <% end %>